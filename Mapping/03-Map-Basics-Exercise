    <script id="candidateCode">
      //function expression
      var getNumberAr = function () {
        return [5, 1, 9, 3, 7];
      };

      var numbers = getNumberAr();
      console.log(numbers);

      var addZeros = function (nums) {
        const newArray = nums.map(tripleZero);
        return newArray;
      };

      var tripleZero = function (aNum) {
        var newNumber = "000" + aNum;

        return newNumber;
      };

      var modifiedNumbers = addZeros(numbers);
      console.log(modifiedNumbers);

      var getPeople = function () {
        var people = [
          { id: 1, name: "Neil" },
          { id: 2, name: "Kharis" },
          { id: 3, name: "Guest" },
        ];
        return people;
      };

      var companyPeople = getPeople();
      console.log(companyPeople);

      var getNames = function (peopleAr) {
        const arrayOfPeople = peopleAr.map((p) => p.name);
        return arrayOfPeople;
      };
